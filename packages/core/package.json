{
  "name": "@artemiskit/core",
  "version": "0.1.2",
  "description": "Core runner, evaluators, and storage for ArtemisKit LLM evaluation toolkit",
  "type": "module",
  "license": "Apache-2.0",
  "author": "code-sensei",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-sensei/artemiskit.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/code-sensei/artemiskit/issues"
  },
  "keywords": [
    "llm",
    "testing",
    "ai",
    "agents",
    "evaluation",
    "openai",
    "anthropic",
    "azure",
    "red-team",
    "security"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly && bun build ./src/index.ts --outdir ./dist --target bun",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "zod": "^3.22.0",
    "yaml": "^2.3.0",
    "fastest-levenshtein": "^1.0.16",
    "nanoid": "^5.0.0",
    "pino": "^8.17.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "pino-pretty": "^10.3.0",
    "typescript": "^5.3.0"
  }
}

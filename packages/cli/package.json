{
  "name": "@artemis/cli",
  "version": "0.0.1",
  "description": "Command-line interface for Artemis LLM testing framework",
  "type": "module",
  "license": "MIT",
  "author": "Cognifai Labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/cognifai-labs/artemis.git",
    "directory": "packages/cli"
  },
  "keywords": ["llm", "testing", "cli", "ai", "agents"],
  "bin": {
    "artemis": "./bin/artemis.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && bun build ./src/index.ts --outdir ./dist --target bun",
    "dev": "bun run ./bin/artemis.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "dependencies": {
    "@artemis/core": "workspace:*",
    "@artemis/adapter-openai": "workspace:*",
    "@artemis/adapter-vercel-ai": "workspace:*",
    "@artemis/reports": "workspace:*",
    "@artemis/redteam": "workspace:*",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "cli-table3": "^0.6.3",
    "inquirer": "^9.2.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.3.0"
  }
}

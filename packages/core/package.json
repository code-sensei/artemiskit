{
  "name": "@artemis/core",
  "version": "0.0.1",
  "description": "Core runner, evaluators, and storage for Artemis LLM testing framework",
  "type": "module",
  "license": "MIT",
  "author": "code-sensei",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-sensei/artemis.git",
    "directory": "packages/core"
  },
  "keywords": ["llm", "testing", "ai", "agents", "evaluation"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly && bun build ./src/index.ts --outdir ./dist --target bun",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "zod": "^3.22.0",
    "yaml": "^2.3.0",
    "fastest-levenshtein": "^1.0.16",
    "nanoid": "^5.0.0",
    "pino": "^8.17.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "pino-pretty": "^10.3.0",
    "typescript": "^5.3.0"
  }
}

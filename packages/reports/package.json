{
  "name": "@artemiskit/reports",
  "version": "0.1.5",
  "description": "HTML report generation for ArtemisKit LLM evaluation toolkit",
  "type": "module",
  "license": "Apache-2.0",
  "author": "code-sensei",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-sensei/artemiskit.git",
    "directory": "packages/reports"
  },
  "bugs": {
    "url": "https://github.com/code-sensei/artemiskit/issues"
  },
  "homepage": "https://artemiskit.vercel.app",
  "keywords": ["llm", "testing", "reports", "html", "evaluation", "artemiskit"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && bun build ./src/index.ts --outdir ./dist --target bun",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "dependencies": {
    "@artemiskit/core": "workspace:*",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.3.0"
  }
}

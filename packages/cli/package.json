{
  "name": "@artemis/cli",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "artemis": "./bin/artemis.ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "dev": "bun run ./bin/artemis.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "dependencies": {
    "@artemis/core": "workspace:*",
    "@artemis/adapter-openai": "workspace:*",
    "@artemis/adapter-vercel-ai": "workspace:*",
    "@artemis/reports": "workspace:*",
    "@artemis/redteam": "workspace:*",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "cli-table3": "^0.6.3",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.3.0"
  }
}
